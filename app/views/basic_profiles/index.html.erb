<h1>Listing basic_profiles</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Maiden name</th>
      <th>Formatted name</th>
      <th>Headline</th>
      <th>Location</th>
      <th>Industry</th>
      <th>Summary</th>
      <th>Specialties</th>
      <th>Picture url</th>
      <th>Public profile url</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @basic_profiles.each do |basic_profile| %>
      <tr>
        <td><%= basic_profile.first_name %></td>
        <td><%= basic_profile.last_name %></td>
        <td><%= basic_profile.maiden_name %></td>
        <td><%= basic_profile.formatted_name %></td>
        <td><%= basic_profile.headline %></td>
        <td><%= basic_profile.location %></td>
        <td><%= basic_profile.industry %></td>
        <td><%= basic_profile.summary %></td>
        <td><%= basic_profile.specialties %></td>
        <td><%= basic_profile.picture_url %></td>
        <td><%= basic_profile.public_profile_url %></td>
        <td><%= basic_profile.user_id %></td>
        <td><%= link_to 'Show', basic_profile %></td>
        <td><%= link_to 'Edit', edit_basic_profile_path(basic_profile) %></td>
        <td><%= link_to 'Destroy', basic_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Basic profile', new_basic_profile_path %>
