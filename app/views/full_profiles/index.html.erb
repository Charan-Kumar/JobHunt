<h1>Listing full_profiles</h1>

<table>
  <thead>
    <tr>
      <th>Associations</th>
      <th>Honors</th>
      <th>Interests</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @full_profiles.each do |full_profile| %>
      <tr>
        <td><%= full_profile.associations %></td>
        <td><%= full_profile.honors %></td>
        <td><%= full_profile.interests %></td>
        <td><%= full_profile.user_id %></td>
        <td><%= link_to 'Show', full_profile %></td>
        <td><%= link_to 'Edit', edit_full_profile_path(full_profile) %></td>
        <td><%= link_to 'Destroy', full_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Full profile', new_full_profile_path %>
